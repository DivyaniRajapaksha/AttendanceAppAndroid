<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.instituteregistration">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">


        <activity android:name=".InstituteStudentListViewer"
            android:parentActivityName=".InstituteManage_Nav">

        <!-- Parent activity meta-data to support 4.0 and lower -->
        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value=".MainActivity" /></activity>



        <activity android:name=".TeacherProfile" />
        <activity android:name=".TeacherLogin" />
        <activity android:name=".RegTeacher" />
        <activity android:name=".Map_Std" />
        <activity
            android:name=".InstituteLogin"
            android:parentActivityName=".DirectPage">

            <!-- Parent activity meta-data to support 4.0 and lower -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity android:name=".AddClass" />
        <activity android:name=".AddStudent" />
        <activity
            android:name=".NewStudentNavigation"
            android:label="@string/title_activity_new_student_navigation"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".StudentReg" />
        <activity android:name=".ViewClass" />
        <activity android:name=".EditClass" />
        <activity
            android:name=".PayFeesSTD"
            android:exported="true" />
        <activity
            android:name=".MarkAttendance"
            android:exported="true" />
        <activity
            android:name=".StudentNavigation"
            android:exported="true"
            android:label="@string/title_activity_student_navigation"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".InstituteManage_Nav"
            android:label="@string/title_activity_institute_manage__nav"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".SignUp" />
        <activity android:name=".DirectPage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Login_Frm2" />
        <activity android:name=".ListAdapter" />
        <activity android:name=".ListItemClass" />
        <activity
            android:name=".InstituteTeacherListViewer"
            android:parentActivityName=".InstituteManage_Nav">

            <!-- Parent activity meta-data to support 4.0 and lower -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".MainActivity"
            android:parentActivityName=".InstituteLogin">

            <!-- Parent activity meta-data to support 4.0 and lower -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/map_key" />
    </application>
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

</manifest>